e.setItem(\"tokens\", token);\r\n        payload['token'] = token\r\n        // console.log(data);\r\n        commit('TOKEN_LOGIN',payload)\r\n        // vue防止跳转到主页后返回到登陆页面,验证通过后用replace（）跳转\r\n        router.replace('./home')\r\n      \r\n}\r\n       \r\n},\r\n    //携带token后来请求数据接口\r\n      async Get_DATA({commit}){\r\n      const result = await this.$tools.GetData();\r\n      commit('TOKEN_DATA',result)\r\n    }\r\n  },\r\n  modules: {}\r\n});\r\n       \r\n     \r\n    \r\n      \r\n"]}]}